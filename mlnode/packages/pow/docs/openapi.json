              "dim": 2048,
              "n_layers": 16,
              "n_heads": 16,
              "n_kv_heads": 16,
              "vocab_size": 8192,
              "ffn_dim_multiplier": 1.3,
              "multiple_of": 1024,
              "norm_eps": 1e-05,
              "rope_theta": 500000.0,
              "use_scaled_rope": true,
